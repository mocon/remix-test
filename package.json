{
  "private": true,
  "name": "remix-test",
  "author": "Myles O'Connor <myles@oconnor.industries>",
  "description": "Trying out Remix Run",
  "license": "UNLICENSED",
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false,
  "scripts": {
    "build": "tailwindcss --output ./app/styles/tailwind.css --config ./config/tailwind.js --minify && remix build",
    "dev": "concurrently \"npm:dev:tailwind\" \"npm:dev:web\"",
    "dev:tailwind": "tailwindcss --output ./app/styles/tailwind.css --config ./config/tailwind.js --watch",
    "dev:web": "node -r dotenv/config node_modules/.bin/remix dev",
    "postinstall": "remix setup node",
    "start": "remix-serve build"
  },
  "dependencies": {
    "@apollo/client": "^3.5.5",
    "@json-rpc-tools/utils": "^1.7.6",
    "@remix-run/react": "^1.0.4",
    "@remix-run/serve": "^1.0.4",
    "@tailwindcss/aspect-ratio": "^0.3.0",
    "@tailwindcss/forms": "^0.3.4",
    "@walletconnect/client": "^1.7.0",
    "algorand-walletconnect-qrcode-modal": "^1.6.1",
    "algosdk": "^1.12.0",
    "axios": "^0.24.0",
    "concurrently": "^6.4.0",
    "dotenv": "^10.0.0",
    "graphql": "^16.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix": "^1.0.4",
    "tailwindcss": "^2.2.19"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.0.4",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "autoprefixer": "^10.4.0",
    "postcss": "^8.3.11",
    "postcss-cli": "^9.0.2",
    "typescript": "^4.1.2"
  }
}
